{
  "name": "@jupyterlab/katex-extension",
  "version": "2.0.0",
  "description": "KaTeX math renderer for JupyterLab",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "katex"
  ],
  "homepage": "https://github.com/jupyterlab/jupyter-renderers",
  "bugs": {
    "url": "https://github.com/jupyterlab/jupyter-renderers/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Project Jupyter",
  "files": [
    "lib/*.d.ts",
    "lib/*.js",
    "schema/*.json",
    "style/*.*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlab/jupyter-renderers.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "prepack": "npm run clean && npm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^2.0.0",
    "@jupyterlab/rendermime": "^2.0.0",
    "@jupyterlab/rendermime-interfaces": "^2.0.0",
    "@jupyterlab/settingregistry": "^2.0.0",
    "katex": "^0.11.1"
  },
  "devDependencies": {
    "@types/katex": "^0.10.0",
    "rimraf": "~2.6.2",
    "typescript": "~3.7.1"
  },
  "jupyterlab": {
    "extension": true,
    "schemaDir": "schema"
  }
}
